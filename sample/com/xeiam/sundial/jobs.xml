<?xml version='1.0' encoding='utf-8'?>
<job-scheduling-data xmlns="http://www.sundial-scheduler.org/xml/JobSchedulingData"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.sundial-scheduler.org/xml/JobSchedulingData classpath:/com/xeiam/sundial/xml/job_scheduling_data.xsd"
  version="2.0">

    <schedule>
        <job>
            <name>SampleJob</name>
            <job-class>com.xeiam.sundial.SampleJob</job-class>
            <job-data-map>
                <entry>
                    <key>Concurrency</key>
                    <value>Y</value>
                </entry>
            </job-data-map>
        </job>
        <trigger>
            <cron>
                <name>SampleJob-Trigger</name>
                <group>CRON</group>
                <job-name>SampleJob</job-name>
                <cron-expression>0/45 * * * * ?</cron-expression>
            </cron>
        </trigger>

    </schedule>
    
</job-scheduling-data>